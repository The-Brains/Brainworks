<div class="panel panel-default full-height">
  <div class="panel-body full-height">
    <h1 class="page-header designer-header">
      {{diagram.title}}
      <a class="btn btn-primary pull-right" tooltip-placement="bottom" tooltip="<%= save %>"><span class="glyphicon glyphicon-floppy-save"></span></a>
    </h1>
    <div class="row full-height" style="overflow: auto;">
      <div class="col-md-4 designer-main-container full-height">
        <accordion close-others="oneAtATime">
          <accordion-group heading="Klassendiagramme" ng-repeat="group in groups">
            <designer-element></designer-element>
          </accordion-group>
        </accordion>
      </div>
      <div class="col-md-8 full-height">
        <div class="full-height designer-container">
          <designer></designer>
        </div>
      </div>
    </div>
  </div>
</div>