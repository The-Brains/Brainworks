<div class="panel panel-default">
  <div class="panel-body">
    <div class="row" ng-repeat-start="diagram in filteredDiagrams">
      <div class="col-md-7">
        <a href="#/diagram/{{diagram._id}}">
          <img class="img-responsive" src="http://placehold.it/700x300" alt="{{diagram.title}}">
        </a>
      </div>
      <div class="col-md-5">
        <h3>{{diagram.title}}</h3>
      </div>
    </div>
    <hr ng-repeat-end>
    <div class="text-center">
      <pagination ng-model="currentPage" total-items="diagrams.length" items-per-page="numPerPage" boundary-links="true" ng-change="filterData()"></pagination>
    </div>
  </div>
</div>